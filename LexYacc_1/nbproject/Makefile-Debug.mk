${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/lexyacc_1: ${OBJECTFILES}
	${MKDIR} -p ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}
	${LINK.c} -o ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/lexyacc_1 ${OBJECTFILES} ${LDLIBSOPTIONS} 

${OBJECTDIR}/y.tab.o: y.tab.c 
	${MKDIR} -p ${OBJECTDIR}
	${RM} $@.d
	$(COMPILE.c) -g -MMD -MP -MF $@.d -o ${OBJECTDIR}/y.tab.o y.tab.c

lex.yy.c: token.l y.tab.h
	@echo Running lex...
	lex token.l

${OBJECTDIR}/lex.yy.o: lex.yy.c 
	${MKDIR} -p ${OBJECTDIR}
	${RM} $@.d
	$(COMPILE.c) -g -MMD -MP -MF $@.d -o ${OBJECTDIR}/lex.yy.o lex.yy.c

.NO_PARALLEL:y.tab.c y.tab.h
y.tab.c y.tab.h: token.y 
	@echo Running yacc...
	yacc -d token.y

